{
    "json_class": "Chef::Environment",
    "chef_type": "environment",
    "name": "vagrant",
    "description": "",
    "override_attributes": {
        "yum-epel": {
            "repositories": ["epel", "nux-dextop"]
        },
        "yum": {
            "nux-dextop": {
                "repositoryid": "nux-dextop",
                "description": "Nux.Ro RPMs for general desktop use",
                "baseurl": "http://li.nux.ro/download/nux/dextop/el7/$basearch/ http://mirror.li.nux.ro/li.nux.ro/nux/dextop/el7/$basearch/",
                "gpgkey": "http://li.nux.ro/download/nux/RPM-GPG-KEY-nux.ro",
                "gpgcheck": true,
                "enabled": true,
                "managed": true
            }
        },
        "java": {
            "install_flavor": "oracle",
            "jdk_version": "8",
            "oracle": {
                "accept_oracle_download_terms": true
            }
        },
        "aem": {
            "version": "5.6.1",
            "download_url": "http://example.com/cq-quickstart-5.6.1.jar",
            "license_url": "http://example.com/license.properties",
            "author": {
                "startup": {
                    "max_attempts": 60
                },
                "jvm_opts": {
                    "-Dsling.run.modes=author,qa": "set run modes",
                    "-Xms1024M -Xmx2048M -XX:MaxPermSize=512M": "prevent the process from forking"
                }
            },
            "publish": {
                "startup": {
                    "max_attempts": 60
                },
                "jvm_opts": {
                    "-Dsling.run.modes=publish,qa": "set run modes",
                    "-Xms1024M -Xmx2048M -XX:MaxPermSize=512M": "prevent the process from forking"
                }
            }
        }
    }
}
